{
  "cli": {
    "version": ">= 16.32.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_APP_ENV": "development",
        "EXPO_PUBLIC_API_BASE_URL": "http://192.168.1.108:3000/api",
        "EXPO_PUBLIC_ENABLE_MOCK_AUTH_FALLBACK": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_APP_ENV": "staging",
        "EXPO_PUBLIC_API_BASE_URL": "https://staging-api.example.com/api",
        "EXPO_PUBLIC_ENABLE_MOCK_AUTH_FALLBACK": "false"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_APP_ENV": "production",
        "EXPO_PUBLIC_API_BASE_URL": "https://api.example.com/api",
        "EXPO_PUBLIC_ENABLE_MOCK_AUTH_FALLBACK": "false"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
