{
  "expo": {
    "name": "DliverScreen",
    "slug": "DliverScreen",
    "version": "1.0.0",
    "orientation": "portrait",
    "jsEngine": "hermes",
    "icon": "./assets/images/icon.png",
    "scheme": "dliverscreen",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.bikienga_hamza.DliverScreen"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "sounds": [
            "./assets/sounds/delivery-alert.wav"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "appEnv": "development",
      "apiBaseUrl": "http://192.168.1.108:3000/api",
      "enableMockAuthFallback": true,
      "eas": {
        "projectId": "092f688f-dbea-489d-b6b4-badeaf3272b8"
      }
    }
  }
}
